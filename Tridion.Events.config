<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
  
    <!-- Define publications where publishing mirroring should be triggered -->
    <add key="SourcePublications" value="600 Example DE-DE"/>
    
    <!-- Define the publications which publishing  -->
